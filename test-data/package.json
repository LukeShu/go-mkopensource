  "scripts": {
    "start": "webpack serve --open --mode development",
    "start-dev-portal": "webpack serve --config webpack.dev-portal.config.js --open --mode development",
    "start-dev-portal:intercept": "webpack serve --mode development --config webpack.dev-portal.config.js --env intercept --compress",
    "start:silent": "webpack serve --mode development",
    "start:intercept": "webpack serve --mode development --env intercept --compress",
    "build": "NODE_ENV=production webpack --mode production",
    "build-dev-portal": "NODE_ENV=production webpack --mode production --config webpack.dev-portal.config.js",
    "postbuild": "rm -rf ./dist/cloud/*.map && rm -rf ./dist/devPortal/*.map",
    "testprodbuild": "serve ./dist",
    "mock": "nodemon",
    "mock:chaos": "nodemon --chaosMode",
    "mock:empty": "nodemon empty",
    "mock:full": "nodemon full",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "clean": "rm -rf ./node_modules && npm install",
    "lint": "npm run lint:config && npm run lint:src && npm run lint:css",
    "lint:config": "eslint .",
    "lint:src": "eslint \"{src,test}/**/*.{js,jsx,ts,tsx}\"",
    "lintfix": "eslint --fix \"{src,test}/**/*.{js,jsx,ts,tsx}\"",
    "lint:css": "stylelint '{src/styles,**/*,*}.css'",
    "lint:css:fix": "stylelint '{src/styles,**/*,*}.css' --fix",
    "precommit": "npm test",
    "prepare": "cd .. && husky install ui/.husky"
  },
  "lint-staged": {
    "\"{src,test}/**/*.{js,jsx,ts,tsx}\"": [
      "eslint \"{src,test}/**/*.{js,jsx,ts,tsx}\" --fix",
      "git add"
    ],
    ".": [
      "eslint . --fix",
      "git add"
    ]
  },
